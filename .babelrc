{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"ie": 11
				},
				"useBuiltIns": "usage",
				"corejs": 3
			}
		]
	],
	"plugins": [
	    [
			"@babel/plugin-proposal-class-properties",
			{
				"loose": true
			}
		],
	    [
			"@babel/plugin-proposal-private-methods",
			{
				"loose": true
			}
		]
	]
}